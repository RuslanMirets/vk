{
	"useTabs": true,
	"printWidth": 100,
	"tabWidth": 2,
	"trailingComma": "all",
	"singleQuote": true,
	"semi": true,
	"jsxSingleQuote": true,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@components/(.*)$",
		"^@providers/(.*)$",
		"^@services/(.*)$",
		"^@assets/(.*)$",
		"^@types/(.*)$",
		"^@hooks/(.*)$",
		"^../(.*)",
		"^./(.*)",
		"(.scss)$"
	],
	"importOrderSeparation": false,
	"importOrderSortSpecifiers": false
}
